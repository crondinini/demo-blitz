{
  "environments": [
    {
      "id": "Test",
      "name": "Test",
      "region": "us-west-1",
      "source": {
        "branch": "main"
      },
      "services": [
        {
          "id": "db",
          "name": "Database",
          "type": "rds",
          "engine": "postgres",
          "engineVersion": "12",
          "instanceSize": "db.t2.micro",
          "storage": 20,
          "private": false
        },
        {
          "id": "test-web",
          "name": "Flight Deck",
          "type": "fargate",
          "domain": "app.flightcontrol.dev",
          "cpu": 1,
          "memory": 2,
          "minInstances": 2,
          "maxInstances": 3,
          "buildCommand": "npm run build",
          "startCommand": "npm run start",
          "envVariables": {
            "APP_ENV": "staging",
            "DATABASE_URL": {
              "fromService": {
                "id": "db",
                "value": "dbConnectionString"
              }
            },
            "GROWTHBOOK_API_KEY": {
              "fromParameterStore": "flightcontrol.growthbook.api.key"
            }
          }
        }
      ]
    }
  ]
}
