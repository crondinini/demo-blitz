{
  "environments": [
    {
      "id": "preview-environment",
      "name": "Test preview environment",
      "source": {
        "pr": true,
        "trigger": "push",
        "filter": {
          "toBranch": "*"
        }
      },
      "services": [
        {
          "id": "preview-db",
          "name": "Database",
          "type": "rds",
          "engine": "postgres",
          "engineVersion": "12",
          "instanceSize": "db.t2.micro",
          "storage": 20,
          "private": false,
          "connectionStringEnvVarName": "DATABASE_URL"
        },
        {
          "id": "preview-test-web",
          "name": "Blitz App",
          "type": "fargate",
          "cpu": 1,
          "memory": 2,
          "maxInstances": 3,
          "buildCommand": "npm run build",
          "startCommand": "npm run start",
          "envVariables": {
            "APP_ENV": "staging",
            "DATABASE_URL": {
              "fromService": {
                "id": "preview-db",
                "value": "dbConnectionString"
              }
            },
            "GROWTHBOOK_API_KEY": "fake-api-key"
          }
        }
      ]
    },
    {
      "id": "Test",
      "name": "Test",
      "region": "us-west-1",
      "source": {
        "branch": "main"
      },
      "services": [
        {
          "id": "db",
          "name": "Database",
          "type": "rds",
          "engine": "postgres",
          "engineVersion": "12",
          "instanceSize": "db.t2.micro",
          "storage": 20,
          "private": false,
          "connectionStringEnvVarName": "DATABASE_URL"
        },
        {
          "id": "test-web",
          "name": "Blitz App",
          "type": "fargate",
          "cpu": 1,
          "memory": 2,
          "maxInstances": 3,
          "buildCommand": "npm run build",
          "startCommand": "npm run start",
          "envVariables": {
            "APP_ENV": "staging",
            "DATABASE_URL": {
              "fromService": {
                "id": "db",
                "value": "dbConnectionString"
              }
            },
            "GROWTHBOOK_API_KEY": "fake-api-key"
          }
        }
      ]
    }
  ]
}
